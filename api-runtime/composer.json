{
    "name": "redbonzai/api-runtime",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "maxbanton/cwh": "^2.0",
        "redbonzai/cloudwatch-logger": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.49",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../libs/cloudwatch-logger",
            "options": {
                "symlink": true
            }
        }
    ],
    "packages": [
        {
            "name": "redbonzai/cloudwatch-logger",
            "description": "A Laravel package for logging to AWS CloudWatch with custom formatting.",
            "type": "library",
            "version": "^1.0",
            "require": {
                "php": "^7.2|^8.0",
                "maxbanton/cwh": "^2.0"
            },
            "autoload": {
                "psr-4": {
                    "Redbonzai\\Logging\\": "src/Logging/"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "./app/",
            "Database\\Factories\\": "./database/factories/",
            "Database\\Seeders\\": "./database/seeders/",
            "Libs\\": "../libs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
